{
    "manifest_version": 3,
    "name": "ECourse Folder Expander",
    "version": "1.0",
    "description": "Expand Folder Functionality For ECourse",
    "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://ecourse.uoi.gr/*"],
        "js": ["content.js"]
      }
    ]
  }
  