{
    "manifest_version": 3,
    "name": "ECourse Folder Expander",
    "version": "1.0",
    "description": "Expand Folder Functionality For ECourse",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://ecourse.uoi.gr/*"],
        "js": ["content.js"]
      }
    ]
  }
  