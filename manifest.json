{
    "manifest_version": 3,
    "name": "E-Course Folder Expander",
    "version": "1.0",
    "description": "Folder Expansion Functionality For E-Course",
    "icons": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
      }
    },
    "permissions": ["activeTab", "storage"],
    "content_scripts": [
      {
        "matches": ["https://ecourse.uoi.gr/*"],
        "js": ["content.js"]
      }
    ]
  }
  